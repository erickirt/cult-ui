{
  "name": "ui",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Jordan-Gilliam",
    "url": "https://twitter.com/Jordan-Gilliam"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "templates/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:cli": "turbo --filter=cult-ui build",
    "build:registry": "pnpm --filter=www build:registry",
    "dev": "turbo run dev --parallel",
    "cli:dev": "turbo --filter=cult-ui dev",
    "cli:start": "pnpm --filter=cult-ui start:dev",
    "www:dev": "pnpm --filter=www dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "preview": "turbo run preview",
    "typecheck": "turbo run typecheck",
    "format:write": "turbo run format:write",
    "format:check": "turbo run format:check",
    "sync:templates": "./scripts/sync-templates.sh \"templates/*\"",
    "prepare": "husky install",
    "check": "turbo lint typecheck format:check",
    "release": "changeset version",
    "pub:beta": "cd packages/cli && pnpm pub:beta",
    "pub:release": "cd packages/cli && pnpm pub:release",
    "test": "turbo run test --force",
    "registry:build": "pnpm --filter=www registry:build"
  },
  "packageManager": "pnpm@9.0.4",
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@emotion/is-prop-valid": "^1.4.0",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/upgrade": "^4.0.6",
    "@typescript-eslint/parser": "^5.59.7",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-turbo": "^2.5.6",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^8.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "turbo": "^2.5.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@types/node": "^20.11.27",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "typescript": "^5.9.2"
  }
}
